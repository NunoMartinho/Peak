{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kafka": {
    "BootstrapServers": [
      "localhost:9092"
    ],
    "Producers": {
      "TrackEvent": {
        "Acks": "Leader",
        "DefaultTopic": "pixel.track.v1",
        "MessageSendMaxRetries": 5
      }
    }
  }
}
